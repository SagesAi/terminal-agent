# OpenAI API Key - Required for the Terminal Agent to function
OPENAI_API_KEY=your_api_key_here

# DeepSeek API Key (可选，如果使用DeepSeek模型)
DEEPSEEK_API_KEY=your_deepseek_api_key_here

# Google Gemini API Key (可选，如果使用Gemini模型)
GOOGLE_API_KEY=your_google_api_key_here

# Anthropic Claude API Key (可选，如果使用Claude模型)
ANTHROPIC_API_KEY=your_anthropic_api_key_here

# VLLM API Key (可选，如果使用VLLM且需要认证)
# VLLM_API_KEY=your_vllm_api_key_here

# LLM Provider 设置 (可选，默认为 "openai")
# 可选值: "openai", "deepseek", "gemini", "anthropic", "ollama", "vllm"
TERMINAL_AGENT_PROVIDER=openai

# LLM Model 设置 (可选)
# 对于 OpenAI: 默认为 "gpt-4"，也可以使用 "gpt-3.5-turbo" 等
# 对于 DeepSeek: 默认为 "deepseek-chat"，也可以使用其他DeepSeek支持的模型
# 对于 Gemini: 默认为 "gemini-pro"
# 对于 Claude: 默认为 "claude-3-opus-20240229"
# 对于 Ollama: 默认为 "llama3"，也可以使用 "llama2", "mistral" 等
# 对于 VLLM: 默认为 "llama-2-7b"，取决于您的VLLM服务器配置
TERMINAL_AGENT_MODEL=gpt-4

# API Base URL (可选，仅在需要自定义API端点时使用)
# TERMINAL_AGENT_API_BASE=https://custom-api-endpoint.com/v1

# Ollama API Base URL (可选，默认为 "http://localhost:11434")
# OLLAMA_API_BASE=http://localhost:11434

# VLLM API Base URL (可选，默认为 "http://localhost:8000")
# VLLM_API_BASE=http://localhost:8000
